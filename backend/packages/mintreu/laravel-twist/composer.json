{
    "name": "mintreu/laravel-twist",
    "description": "Twist For Your Laravel App",
    "keywords": [
        "mintreu",
        "laravel",
        "laravel-twist"
    ],
    "homepage": "https://github.com/mintreu/laravel-twist",
    "support": {
        "issues": "https://github.com/mintreu/laravel-twist/issues",
        "source": "https://github.com/mintreu/laravel-twist"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Krishanu",
            "email": "36711704+krishzzi@users.noreply.github.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",

        "spatie/laravel-package-tools": "^1.15.0"
    },
    "require-dev": {

    },
    "autoload": {
        "psr-4": {
            "Mintreu\\LaravelTwist\\": "src/",
            "Mintreu\\LaravelTwist\\Database\\Factories\\": "database/factories/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Mintreu\\LaravelTwist\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": "@php ./vendor/bin/testbench package:discover --ansi",
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Mintreu\\LaravelTwist\\LaravelTwistServiceProvider"
            ],
            "aliases": {
                "LaravelTwist": "Mintreu\\LaravelTwist\\Facades\\LaravelTwist"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
